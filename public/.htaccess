<IfModule mod_rewrite.c>
  RewriteEngine On


   RewriteCond %{SERVER_PORT} 80
   RewriteRule ^(.*)$ https://www.kolshop.com.tw/kol_kitchen/$1 [R,L]
 
   RewriteCond %{HTTP_HOST} !^www\. [NC]
   RewriteRule ^(.*)$ https://www.kolshop.com.tw/kol_kitchen/$1 [R=301,L]
  
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /kol_kitchen/index.html [L]
</IfModule>